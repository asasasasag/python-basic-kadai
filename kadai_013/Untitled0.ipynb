        "def calculate_total_price(price, tax_rate):\n",
        "    total_price = price * (1 + tax_rate)\n",
        "    return total_price\n",
        "\n",
        "# 入力欄\n",
        "price = 1000  # 商品の金額\n",
        "tax_rate = 0.10  # 消費税率（10%）\n",
        "total_price = calculate_total_price(price, tax_rate)\n",
        "print(f\"税込み価格: {total_price:.2f}円\")\n"
